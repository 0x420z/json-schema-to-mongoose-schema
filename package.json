  "keywords": [
    "json",
    "schema",
    "mongoose",
    "json schema",
    "mongoose schema",
    "json to mongoose",
    "json schema to mongoose",
    "mongoose schema builder",
    "mongoose schema function"
  ],
  "contributors": [
    {
      "name": "Jean M. Lescure",
      "email": "opensource@jeanlescure.io",
      "url": "http://jeanlescure.io/"
    }
  ],
  "main": "dist/json-schema-to-mongoose-schema.js",
  "typings": "dist/json-schema-to-mongoose-schema.d.ts",
  "scripts": {
    "dev": "./scripts/dev",
    "test": "./scripts/test",
    "build": "./scripts/build",
    "release": "release-it"
  },
  "release-it": {
    "git": {
      "changelog": "auto-changelog --stdout -l false -u -t ./assets/changelog-compact.hbs"
