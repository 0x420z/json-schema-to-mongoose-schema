  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.4",
    "auto-changelog": "^2.3.0",
    "esbuild": "^0.13.9",
    "jest": "^27.3.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.12",
    "refup": "^1.1.0",
    "release-it": "^14.11.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "lodash": ">=4.0.0",
    "mongoose": ">=5.0.0"
  }
